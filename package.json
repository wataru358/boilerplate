  "jest": {
    "moduleFileExtensions": [
      "tsx",
      "ts",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|ico|html)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|scss|sass|less)$": "<rootDir>/src/__mocks__/objectMock.js",
      "^root(.*)$": "<rootDir>$1",
      "^src(.*)$": "<rootDir>/src$1",
      "^config(.*)$": "<rootDir>/src/config$1",
      "^shared(.*)$": "<rootDir>/src/shared$1"
    },
    "setupFiles": [
      "<rootDir>/node_modules/babel-polyfill/browser.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/__tests__/configureJest.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testRegex": "\\/__tests__\\/.*\\.test\\.(js|tsx)$",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(tsx|ts)$": "<rootDir>/node_modules/babel-jest"
    }
  }
