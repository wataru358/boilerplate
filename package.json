  "scripts": {
    "analyze": "BUNDLE_ANALYZE=both next build",
    "build": "next build",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:record": "cypress run --record --key /*yourke*/",
    "cy:citest": "start-server-and-test start http://localhost:8888 cy:run",
    "dev": "node server.js",
    "format": "prettier --write '{pages,src}/**/*.{tsx,ts,js,scss}'",
    "lint": "eslint --ext .tsx .",
    "lint:fix": "eslint --fix --quiet --ext .tsx .",
    "precommit": "lint-staged",
    "start": "NODE_ENV=production node server.js",
    "tsc": "tsc --version && tsc",
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.tsx": [
      "prettier --write",
      "git add"
    ]
  },
